/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@services/*": [
        "./app/core/services/*"
      ],
      "@model/*": [
        "./app/core/model/*"
      ],
      "@shared/*":[
        "./app/shared/*"
      ],
      "@utile/*":[
        "./app/core/utile/*"
      ],
      "@environments/*":[
        "./environments/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]

}
